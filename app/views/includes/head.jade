head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    meta(name='viewport', content='width=device-width,initial-scale=1')

    title= appName + ' - ' + title
    meta(http-equiv='Content-type', content='text/html;charset=UTF-8')
    meta(name='keywords', content='node.js, express.js, mongoose, mongodb, angularjs')
    meta(name='description', content='Kalel - Nodejs framework')

    meta(property='fb:app_id', content='APP_ID')
    meta(property='og:title', content='#{appName} - #{title}')
    meta(property='og:description', content='Kalel framework - MongoDB, ExpressJs, AngularJs, NodeJs')
    meta(property='og:type', content='website')
    meta(property='og:url', content='APP_URL')
    meta(property='og:image', content='APP_LOGO')
    meta(property='og:site_name', content='Kalel - Framework')

    link(rel='stylesheet', href='/css/lib/bootstrap.min.css')
    link(rel='stylesheet', href='/css/lib/bootstrap-responsive.min.css')
    link(rel='stylesheet', href='/css/lib/bootstrap-datepicker.css')
    link(rel='stylesheet', href='/css/common.css')

    link(rel='stylesheet', href='/css/views/index.css')